program tampil;
uses crt;
  const
    kota='Ambon';
  var
    nama:string='Junaidi';
    umur:integer=17;
  begin
    clrscr;

    write('Nama : ');
    writeln(nama);

    write('Umur : ');
    writeln(umur);

    write('Kota : ');
    write(kota);

    write('IPK :');
    writeln(4);

  readln;
  end.